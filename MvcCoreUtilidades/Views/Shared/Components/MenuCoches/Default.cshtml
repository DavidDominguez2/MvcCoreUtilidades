@model List<Coche>

<div class="dropdown">
  <button class="btn btn-secondary dropdown-toggle" type="button" data-bs-toggle="dropdown" aria-expanded="false">
    Menú Coches
  </button>
  <ul class="dropdown-menu">
        @foreach(Coche car in Model) {
            <li><a class="dropdown-item" asp-controller="Coches" asp-action="Details" asp-route-idcoche="@car.IdCoche">@car.Modelo</a></li>
        }
  </ul>
</div>